<meiQiReferrer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="orderList.xsd">
	<sections>
		<sectionName>MQTableBaseSection</sectionName>
		<sectionItems>
			<viewName>MQTitleTableViewCell</viewName>
			<data>
				<leftFirstValue>收货人</leftFirstValue>
				<leftSecondValue>${OrderItem.consignee.name}</leftSecondValue>
				<leftThirdValue>${OrderItem.consignee.phone((\d{3})(\d{4})(\d{4}),$1****$3)}</leftThirdValue>
				<rightRedValue>配送完成</rightRedValue>
			</data>
		</sectionItems>
		<sectionItems>
			<viewName>MQMultiGoodsTableViewCell</viewName>
			<data>
				<imgURLStr1>${OrderItem.carts[0].goods.cover}</imgURLStr1>
				<imgURLStr2>${OrderItem.carts[1].goods.cover}</imgURLStr2>
				<imgURLStr3>${OrderItem.carts[2].goods.cover}</imgURLStr3>
				<imgURLStr4>${OrderItem.carts[3].goods.cover}</imgURLStr4>
				<imgURLStr5>${OrderItem.carts[4].goods.cover}</imgURLStr5>
			</data>
		</sectionItems>
		<sectionItems>
			<viewName>MQButtonActionTableViewCell</viewName>
			<data>
				<leftFirstValue>实付款:</leftFirstValue>
				<lelftSecondValue>￥${OrderItem.amount}</lelftSecondValue>
			</data>
			<actions>
			<actionName>confirmShipCommand</actionName>
				<sender>btnRed</sender>
				<title>确认收货</title>
				<actionValue>${OrderItem.orderId}</actionValue>
			</actions>
			<actions/>
		</sectionItems>
		<actions>
			<actionName>showOrderDetail</actionName>
			<sender>self</sender>
			<title></title>
			<actionValue>${OrderItem.orderId}</actionValue>
		</actions>
		<actions/>
	</sections>
	<sections/>
</meiQiReferrer>
